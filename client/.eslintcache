lassName=\"py-3 border-bottom row align-items-center\">\r\n      <div className=\"col-1\">{indx}</div>\r\n      <div className=\"col-2\">{name}</div>\r\n      <div className=\"col-2\">{lifespan}</div>\r\n      <div className=\"col-3\">\r\n        <Link\r\n          to={`/authors/${id}`}\r\n          className=\"btn me-2 btn-sm btn-outline-info\"\r\n        >\r\n          View\r\n        </Link>\r\n        <Link\r\n          to={`/authors/${id}/edit`}\r\n          className=\"btn me-2 btn-sm btn-outline-warning\"\r\n        >\r\n          Edit\r\n        </Link>\r\n        <Link\r\n          onClick={handleDelete}\r\n          className=\"btn me-2 btn-sm btn-outline-danger\"\r\n        >\r\n          Delete\r\n        </Link>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default AuthorListItem;\r\n","C:\\Users\\lin\\Documents\\1_study\\practice\\library-management-nodejs\\client\\src\\pages\\authors\\authorListHeading.js",[],"C:\\Users\\lin\\Documents\\1_study\\practice\\library-management-nodejs\\client\\src\\pages\\authors\\author-edit.jsx",["187"],"import React, { useCallback, useEffect, useState } from \"react\";\r\nimport { getAuthorDetailByID } from \"../../actions/authors\";\r\nimport MainLayout from \"../../components/mainLayout\";\r\nimport { useDispatch, useSelector } from \"react-redux\";\r\nimport { Link, useParams } from \"react-router-dom\";\r\n\r\nconst AuthorDetail = () => {\r\n  const { id } = useParams();\r\n  const dispatch = useDispatch();\r\n  const stableDispatch = useCallback(() => dispatch(getAuthorDetailByID(id)), [\r\n    dispatch,\r\n    id,\r\n  ]);\r\n  // set up local state\r\n  const initialInputs = {\r\n    fName: { value: \"\", error: \"\" },\r\n    lName: { value: \"\", error: \"\" },\r\n    dob: { value: \"\", error: \"\" },\r\n    dod: { value: \"\", error: \"\" },\r\n  };\r\n  const [inputs, setInputs] = useState(initialInputs);\r\n  // execute upon loaded\r\n  useEffect(() => {\r\n    stableDispatch();\r\n  }, [stable